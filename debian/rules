#! /usr/bin/make -f
#
DH_ALWAYS_EXCLUDE:=CVS:.svn:.svnignore:.hg:.hgignore:.git

export LDFLAGS = "-Wl,--as-needed" "-Wl,--no-undefined" "-Wl,-z,defs"
build-arch: build-stamp
build-indep: build-stamp

override_dh_auto_configure:
	dh_auto_configure -Skde -- -DCMAKE_USE_RELATIVE_PATHS=ON

override_dh_install:
	dh_install --autodest --list-missing

%:
	dh $@ --parallel --dbg-package=kpart-webkit-dbg --with kde

.PHONY: override_dh_auto_test

